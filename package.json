    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "electron-builder": "^22.10.5",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "gulp": "^4.0.2",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.4",
